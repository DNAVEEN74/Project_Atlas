      "year": 2024,
      "shift": "26/09/2024 9:00 AM - 10:00 AM"
    },
    "solution": {
      "steps": [
        "Given:",
        "• Curved surface area = 550 cm²",
        "• Radius (r) = 7 cm",
        "• π = 22/7",
        "",
        "Formula: Curved surface area of cone = πrl (where l = slant height)",
        "",
        "Step 1: Find slant height",
        "πrl = 550",
        "(22/7) × 7 × l = 550",
        "22 × l = 550",
        "l = 550/22 = 25 cm",
        "",
        "Step 2: Find base area",
        "Base area = πr²",
        "Base area = (22/7) × 7²",
        "Base area = (22/7) × 49",
        "Base area = 22 × 7 = 154 cm²",
        "",
        "Step 3: Calculate total surface area",
        "Total surface area = Curved surface area + Base area",
        "Total surface area = 550 + 154 = 704 cm²"
      ],
      "answer": "704.00 cm²",
      "verification": "CSA = πrl = (22/7) × 7 × 25 = 550 ✓. Base area = πr² = (22/7) × 49 = 154. TSA = 550 + 154 = 704 cm² ✓"
    }
  },
